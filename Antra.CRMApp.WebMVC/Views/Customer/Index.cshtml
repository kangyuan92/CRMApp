@model IEnumerable<CustomerModel>
<a asp-action="Create" asp-controller="Customer" class="btn bg-success" >Add Customer</a>
<table class="table table-striped">
  <thead>
      <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Title</th>
          <th>Address</th>
          <th>City</th>
          <th>RegionId</th>
          <th>PostalCode</th>
          <th>Country</th>
          <th>Phone</th>
          <th>Option</th>
      </tr>
  </thead>
  <tbody>
      @foreach(var m in Model)
        {
            <tr>
                <td>@m.Id</td>
                <td>@m.Name</td>
                <td>@m.Title</td>
                <td>@m.Address</td>
                <td>@m.City</td>
                <td>@m.RegionId</td>
                <td>@m.PostalCode</td>
                <td>@m.Country</td>
                <td>@m.Phone</td>
                <td>
               <span>
               <a asp-controller="Customer" asp-action="Edit" asp-route-id="@m.Id" >Edit</a>
               </span>
               <span>
               <a asp-controller="Customer" asp-action="Delete" asp-route-id="@m.Id" >Delete</a>
               </span>
               </td>
            </tr>
        }
    </tbody>
</table>
