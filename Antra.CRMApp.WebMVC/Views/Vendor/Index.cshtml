@model IEnumerable<VendorModel>
<a asp-action="Create" asp-controller="Vendor" class="btn bg-success" >Add Vendor</a>
<table class="table table-striped">
  <thead>
      <tr>
          <th>Id</th>
          <th>Name</th>
          <th>City</th>
          <th>Country</th>
          <th>Mobile</th>
          <th>EmailId</th>
          <th>IsActive</th>
          <th>Option</th>
      </tr>
  </thead>
  <tbody>
      @foreach(var m in Model)
        {
            <tr>
                <td>@m.Id</td>
                <td>@m.Name</td>
                <td>@m.City</td>
                <td>@m.Country</td>
                <td>@m.Mobile</td>
                <td>@m.EmailId</td>
                <td>@m.IsActive</td>
                <td>
               <span>
               <a asp-controller="Vendor" asp-action="Edit" asp-route-id="@m.Id" >Edit</a>
               </span>
               <span>
               <a asp-controller="Vendor" asp-action="Delete" asp-route-id="@m.Id" >Delete</a>
               </span>
               </td>
            </tr>
        }
    </tbody>
</table>
